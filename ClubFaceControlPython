ageList = []

while True:
    security = int(input("You are about to enter the sickest club in the world, but what's your age first: "))
    if security >= 18:
        ageList.append(security)
        print("Welcome, enjoy your night.")
    elif security < 18:
        ageList.append(security)
        print("Come back when you are 18 or older.")
    if security == 0:
        break

P = (len(ageList))
print("There were", str(P-1), "people in the line.")
